{
	"manifest_version": 2,
	"name": "Codam's Monitoring System Progress for Intra 42",
	"short_name": "Monitoring System Progress for Intra",
	"version": "0.1.0",
	"version_name": "0.1.0",
	"description": "Replaces the Black Hole box on Intra 42 with a display of your current progress for Codam's evaluation system",
	"icons": {},
	"permissions": [ "*://profile.intra.42.fr/", "*://*.intra.42.fr/users/*" ],
	"content_scripts": [
		{
			"matches": [ "*://profile.intra.42.fr/", "*://*.intra.42.fr/users/*" ],
			"js": ["monit.js"],
			"css": ["monit.css"],
			"run_at": "document_end"
		}
	]
}
